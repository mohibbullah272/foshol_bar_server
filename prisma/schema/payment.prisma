model payment {
    id Int @id @default(autoincrement())
    user User @relation(fields: [userId], references: [id])
    userId Int
    project Project @relation(fields: [projectId], references: [id])
    projectId Int
    method PaymentMethods
    amount Int
    status paymentStatus

}


enum PaymentMethods {
    BKASH
    NAGAD
    ROCKET
    UPAY
    BANK
    UCB
}

enum paymentStatus {
    APPROVED
    PENDING
    
}